{
    "name": "chris-doehring/ssh",
    "description": "A lightweight package to execute commands over an SSH connection, based on `spatie/ssh`, with php 5.6 compatibility.",
    "keywords": [
        "ssh",
        "php",
        "5.6"
    ],
    "homepage": "https://github.com/chris-doehring/ssh",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Chris Doehring",
            "email": "info@chrisdoehring.de",
            "homepage": "https://chrisdoehring.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6.40",
        "symfony/process": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/var-dumper": "^3.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ChrisDoehring\\Ssh\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChrisDoehring\\Ssh\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
